{
  "name": "fanyi-cli",
  "version": "1.0.0",
  "description": "A simple command line translation tool",
  "bin": {
    "fanyi": "./bin/fanyi.js"
  },
  "type": "module",
  "scripts": {
    "dev": "node server/index.js",
    "web": "node server/index.js",
    "start": "node server/index.js",
    "start:web": "node server/index.js",
    "config": "node bin/fanyi.js config",
    "build": "cd web && npm run build",
    "build:web": "cd web && npm run build",
    "dev:web": "cd web && npm run dev",
    "preview:web": "cd web && npm run preview",
    "fanyi": "node bin/fanyi.js"
  },
  "dependencies": {
    "axios": "^1.7.0",
    "commander": "^12.0.0",
    "pake-cli": "^3.6.3",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "mongodb": "^6.3.0"
  }
}